import "std"

struct Person {
    fn new(name, age) {
        self.name = name
        self.age = age
    }

    fn greet(self) {
        console.log(self)
    }

    fn a_static_method() {
        console.log("ayo dude")
    }

    async fn grow_up(self) {
        self.age = 1 + self.age
    }
}

// javascript
javascript{
    const person = new Person("jordan", 22);
    person.greet();

    Person.a_static_method();
}

// easyjs
new_person = Person("Evelyn", 19)
new_person.greet()
Person.a_static_method()